FillPropertyFileQueue  ROUTINE
   DATA

   CODE
      FREE(PropertyFileQueue)
      !get user properties
      szSearchFolder = svSpecialFolder.GetDir(SV:CSIDL_APPDATA, 'Devuna' & '\' & 'KSS\')
      bReadOnly = FALSE
      DO GetFilenames

      !get system properties
      szSearchFolder = LONGPATH() & '\'
      bReadOnly = TRUE
      DO GetFileNames

      GET(PropertyFileQueue,1)
