AddEditCommand    ROUTINE
   DATA
i     LONG
j     LONG

   CODE
      UPDATE()
      IF thisEditCommand <> ''
         j = RECORDS(EditorQueue)
         LOOP i = 1 TO j
            GET(EditorQueue,i)
            IF EditorQueue.szValue = thisEditCommand
               BREAK
            END
         END
         IF i > j
            IF j = MAXMRU
               GET(EditorQueue,j)
               DELETE(EditorQueue)
            END
            EditorQueue.szValue = thisEditCommand
            ADD(EditorQueue,+EditorQueue.szValue)
         END
      END
[SOURCE]
